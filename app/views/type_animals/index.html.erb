<p id="notice"><%= notice %></p>

<h1>Type Animals</h1>

<table>
  <thead>
    <tr>
      <th>Libelle</th>
      <th>Code</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_animals.each do |type_animal| %>
      <tr>
        <td><%= type_animal.libelle %></td>
        <td><%= type_animal.code %></td>
        <td><%= link_to 'Show', type_animal %></td>
        <td><%= link_to 'Edit', edit_type_animal_path(type_animal) %></td>
        <td><%= link_to 'Destroy', type_animal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type Animal', new_type_animal_path %>
